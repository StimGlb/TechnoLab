{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules/.vite dist",
    "clean:all": "rm -rf node_modules node_modules/.vite dist package-lock.json",
    "fresh": "npm run clean && npm run dev",
    "reinstall": "npm run clean:all && npm install && npm run dev",
    "migrate:tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "create-icon": "npx @iconify/json --icons fa-solid --save",
    "upgrade:deps": "npx npm-check -u",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "setup": "npm install && npm run build && npm run lint:fix",
    "release": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
